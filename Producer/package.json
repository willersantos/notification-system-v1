    "scripts": {
        "build": "nest build",
        "start": "nest start",
        "lint": "npx eslint .",
        "lint:fix": "npx eslint . --fix",
        "module": "nest g module",
        "service": "nest g service",
        "decorator": "nest g d",
        "repository": "nest g repository",
        "controller": "nest g controller",
        "start:local": "cross-env NODE_ENV=local nest start --watch",
        "start:dev": "nest start NODE_ENV=dev --watch",
        "start:debug": "node nest start --debug --watch",
        "start:prod": "node dist/main",
        "typeorm": "npx typeorm",
        "typeorm-generate": "npm run build && npx typeorm -d dist/shared/database/database.js",
        "migration:create:local": "cross-env NODE_ENV=local npm run typeorm -- migration:create src/shared/database/migrations/",
        "migration:create": "npm run typeorm -- migration:create src/shared/database/migrations/",
        "migration:generate:local": "cross-env NODE_ENV=local npm run typeorm-generate -- migration:generate src/shared/database/migrations/",
        "migration:generate": "npm run typeorm-generate -- migration:generate src/shared/database/migrations/",
        "migration:run:local": "cross-env NODE_ENV=local npm run typeorm -- migration:run -d dist/shared/database/database.js",
        "migration:run": "npm run typeorm -- migration:run -d dist/shared/database/database.js",
        "migration:revert": "npm run typeom -- migration:revert",
        "ts-node": "cross-env NODE_ENV=local ts-node ./node_modules/typeorm/cli.js migration:run -d dist/shared/database/database.js",
        "typeorm-cache-clear": "typeorm cache:clear"
    },
